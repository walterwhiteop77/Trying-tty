# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here

# Channel IDs for video categories (use negative IDs for channels)
CATEGORY_1_CHANNEL=-1001234567890
CATEGORY_2_CHANNEL=-1001234567891  
CATEGORY_3_CHANNEL=-1001234567892
CATEGORY_4_CHANNEL=-1001234567893

# Log channel ID
LOG_CHANNEL_ID=-1001234567894

# Admin user IDs (comma separated)
ADMIN_IDS=123456789,987654321

# Auto-delete settings (optional)
# Set to 'false' to disable auto-deletion of videos
AUTO_DELETE_VIDEOS=true
# Minutes before auto-deletion (default: 10)
AUTO_DELETE_MINUTES=10